{
  "rest": {
    "RestGenerator": {
      "uri": "http://host.docker.internal:8000/generate",
      "method": "post",
      "headers": {
        "Content-Type": "application/json"
      },
      "req_template_json_object": {
        "prompt": "$INPUT"
      },
      "response_json": true,
      "response_json_field": "response",
      "request_timeout": 1080
    }
  }
}
